#打印九九乘法表
i = 1
while i <= 9:
    j = 1
    while j < i:
        print("%d * %d = %d"%(j,i,j*i),end = "   ")
        j += 1
    print("%d * %d = %d "%(j,i,j*i))
    i += 1

# #打印数行小星星，每行依次递增
# i = 0
# while i <= 5:
#     print("*"*i)
#     i += 1

row = 1

while row <= 5:
    col = 1
    while col < row :
        print("*",end = "")
        col += 1
    print("*")
    row += 1







