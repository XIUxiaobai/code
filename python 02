import random
#玩家出拳
player = int(input("石头（1）布（2）剪刀（3）玩家请出拳： "))
#电脑出拳
computer = random.randint(1,3)
print("电脑出得拳是：%d" % computer)
#判断胜负
if player < 1 or player > 3:
    print("您输入的数值错误")
else:
    if ((player == 1 and computer == 3)
            or (player == 2 and computer == 1)
            or (player == 3 and computer == 2)):
        print("玩家胜利")
    elif player == computer:
        print("平局")
    else:
        print("电脑胜利")
  
  
  last_name ="张"
first_name = "三"
print(last_name + first_name)
print(last_name * 10 + first_name *10)
print((last_name+first_name)*10)
#用户输入的任何内容在python看来都是字符串
#input输入的内容都被看作是字符串类型的
password = input("请输入银行密码：")
print(password)
age = input("请输入年龄")
Agee = int(age)
print(type(Agee))

