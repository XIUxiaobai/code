last_name ="张"
first_name = "三"
print(last_name + first_name)
print(last_name * 10 + first_name *10)
print((last_name+first_name)*10)
#用户输入的任何内容在python看来都是字符串
#input输入的内容都被看作是字符串类型的
password = input("请输入银行密码：")
print(password)
age = input("请输入年龄")
Agee = int(age)
print(type(Agee))

#根据要求输入价格
price_str = input("请输入价格")
#根据要求输入重量
weight_str = input("请输入重量")
price = float(price_str)
weight = float(weight_str)
money = price * weight
print(money)



